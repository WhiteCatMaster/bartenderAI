{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "script"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-undef": "error",
        "eqeqeq": "error",
        "semi": ["error", "always"]
    },
    "globals": {
        "window": "readonly",
        "document": "readonly",
        "WebSocket": "readonly"
    }
}